  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@cms-headless/domain/(.*)": "<rootDir>../../../libs/domain/src/$1",
    "@cms-headless/domain": "<rootDir>../../../libs/domain/src"
  }
}
